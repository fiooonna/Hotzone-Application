(null)\n  const onClick = props.onClick\n  useEffect(() => {\n    function handleClickOutside(event) {\n      if (ref.current && !ref.current.contains(event.target)) {\n        onClick(true)\n      }\n    }\n    document.addEventListener(\"mousedown\", handleClickOutside);\n    return () => {\n      document.removeEventListener(\"mousedown\", handleClickOutside);\n    };\n  }, [ref])\n  return <div ref={ref}>{props.children}</div>;\n}\n\nClickAway.propTypes = {\n  children: PropTypes.element.isRequired\n};\n\nexport default ClickAway\n","import React, { useEffect, useState } from \"react\"\nimport { createUseStyles } from \"react-jss\"\nimport cn from \"classnames\"\n\nconst useLineInputStyle = createUseStyles({\n  root: {\n    display: \"flex\",\n    alignItems: \"center\",\n    height: 30,\n    width: \"100%\",\n    border: \"1px solid black\"\n  },\n  input: {\n    width: \"100%\",\n    border: \"0px none\",\n    outline: \"none\",\n    \"& :focus\": {\n      outline: \"none\",\n    },\n    lineHeight: \"28px\",\n  },\n})\n\nconst LineInput = (props) => {\n  const classes = useLineInputStyle()\n  return (\n    <div className={classes.root}>\n      <input type={props.type? props.type : \"input\"} className={classes.input} required onChange={props.onChange}/>\n    </div>\n  )\n}\n\nexport default LineInput","import React, { useEffect, useState } from \"react\"\nimport { createUseStyles } from \"react-jss\"\nimport cn from \"classnames\"\n\nconst useDateInputStyle = createUseStyles({\n  root: {\n    display: \"flex\",\n    alignItems: \"center\",\n    height: 30,\n    width: \"100%\",\n    border: \"1px solid black\"\n  },\n  input: {\n    width: \"100%\",\n    border: \"0px none\",\n    outline: \"none\",\n    \"& :focus\": {\n      outline: \"none\",\n    },\n    lineHeight: \"28px\",\n  },\n})\n\nconst DateInput = (props) => {\n  const classes = useDateInputStyle()\n  return (\n    <div className={classes.root}>\n      <input type={props.type? props.type : \"date